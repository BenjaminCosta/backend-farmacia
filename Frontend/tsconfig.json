{
  "compilerOptions": {
    "noEmit": false,
    "outDir": "src-js",
    "declaration": false,
    "emitDeclarationOnly": false,
    "jsx": "preserve",
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "strictNullChecks": false
  },
  "include": ["src"]
}
